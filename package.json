    "mathjax3",
    "react",
    "equation",
    "latex",
    "tex",
    "asciimath",
    "mathml"
  ],
  "author": "Alexandre Nunes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asnunes/notion-page-to-html/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/asnunes/mathjax3-react"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "simple-load-script": "^1.0.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "gh-pages": "^3.2.3",
    "react-scripts": "^4.0.3",
    "install-peers": "^1.0.3"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
