{
  "name": "mathjax3-react",
  "version": "1.0.11",
  "private": false,
  "main": "dist/index.js",
  "homepage": "http://asnunes.github.io/mathjax3-react",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
